=content_for :header do
  = render :partial => "layouts/event_list_header"

%ul.events
  - @agent.events.each do |event|
    %li.event
      .happen-time
        = event.happen_time
      .event-type
        =link_to event.event_type, agent_event_path(@agent, event)
      .mode
        = image_tag("#{event.mode}.png")
